#: models.py:39
msgid "Display Correctness?"
msgstr "བཀྲམ་སྟོན་ཡང་དག་ཡིན་ནམ།"

#: models.py:41
msgid ""
"This is a flag that indicates if the indicator icon should be displayed "
"after a student enters their response"
msgstr ""
"འདི་ནི་སློབ་མ་ཞིག་གིས་ལན་འདེབས་བྱས་རྗེས་མཚོན་རྟགས་ཀྱི་འདྲ་སྐུ་དེ་མངོན་དགོས་མིན་སྟོན་པའི་དར་ཆ་ཞིག་རེད།"

#: models.py:49
msgid "Display Other Student Responses"
msgstr "སློབ་མ་གཞན་གྱི་ལན་འདེབས་བསྟན་པ།"

#: models.py:51
msgid ""
"This will display other student responses to the student after they submit "
"their response."
msgstr "འདིས་སློབ་མ་གཞན་གྱིས་ལན་འདེབས་ཕུལ་རྗེས་སློབ་མ་ལ་ལན་འདེབས་བསྟན་ཐུབ།"

#: models.py:60
msgid "System selected answers to give to students"
msgstr "མ་ལག་བདམས་ནས་སློབ་མར་སྤྲོད་པའི་ལན་འདེམས།"

#: models.py:63
msgid "Display Name"
msgstr "འཆར་སྟོན་བྱ་རྒྱུའི་མིང་།"

#: models.py:65
msgid "This is the title for this question type"
msgstr "འདི་ནི་དྲི་བ་འདིའི་རིགས་ཀྱི་ཁ་བྱང་ཡིན།"

#: models.py:71
msgid "Full-Credit Key Phrases"
msgstr "བུ་ལོན་ཆ་ཚང་གི་གལ་ཆེའི་ཚིག་གྲུབ།"

#: models.py:73
msgid ""
"This is a list of words or phrases, one of which must be present in order "
"for the student's answer to receive full credit"
msgstr ""
"འདི་ནི་མིང་ཚིག་དང་ཚིག་གྲུབ་ཀྱི་ཐོ་གཞུང་ཞིག་ཡིན་ཞིང་། "
"སློབ་མའི་ལན་ཆ་ཚང་ཐོབ་ཆེད་མིང་ཚིག་གཅིག་ཡོད་དགོས།"

#: models.py:81
msgid "Half-Credit Key Phrases"
msgstr "བུ་ལོན་ཕྱེད་ཀའི་གལ་ཆེའི་ཚིག་གྲུབ།"

#: models.py:83
msgid ""
"This is a list of words or phrases, one of which must be present in order "
"for the student's answer to receive half credit"
msgstr ""
"འདི་ནི་མིང་ཚིག་དང་ཚིག་གྲུབ་ཀྱི་ཐོ་གཞུང་ཞིག་ཡིན་ཞིང་། "
"སློབ་མའི་ལན་ཕྱེད་ཀ་ཐོབ་ཆེད་དུ་དེའི་ནང་ནས་གཅིག་ཡོད་དགོས།"

#: models.py:91
msgid "Maximum Number of Attempts"
msgstr "ཚོད་ལྟ་མང་ཤོས།"

#: models.py:93
msgid ""
"This is the maximum number of times a student is allowed to attempt the "
"problem"
msgstr "འདི་ནི་སློབ་མ་ཞིག་གིས་དཀའ་ངལ་དེ་ཚོད་ལྟ་བྱེད་ཆོག་པའི་ཐེངས་མང་ཤོས་ཡིན།"

#: models.py:101
msgid "Maximum Word Count"
msgstr "མིང་ཚིག་མང་ཤོས།"

#: models.py:103
msgid "This is the maximum number of words allowed for this question"
msgstr "འདི་ནི་དྲི་བ་འདིའི་ཆེས་མཐོའི་ཚིག་གྲངས་ཡིན།"

#: models.py:111
msgid "Minimum Word Count"
msgstr "མིང་ཚིག་ཉུང་ཤོས།"

#: models.py:113
msgid "This is the minimum number of words required for this question"
msgstr "འདི་ནི་དྲི་བ་འདི་ལ་དགོས་པའི་ཚིག་གྲངས་ཉུང་ཤོས་ཡིན།"

#: models.py:121
msgid "Prompt"
msgstr "དུས་བཀག"

#: models.py:123
msgid ""
"This is the prompt students will see when asked to enter their response"
msgstr "འདི་ནི་སློབ་མ་ཚོས་ལན་འདེབས་བྱེད་སྐབས་མཐོང་རྒྱུ་ཡིན།"

#: models.py:131
msgid "Submission Received Message"
msgstr "ཕུལ་བའི་འཕྲིན་ཡིག་འབྱོར།"

#: models.py:133
msgid "This is the message students will see upon submitting their response"
msgstr "འདི་ནི་སློབ་མ་ཚོས་ལན་འདེབས་བྱས་རྗེས་མཐོང་བའི་བརྡ་འཕྲིན་ཡིན།"

#: models.py:140
msgid "Weight"
msgstr "སྦྱངས་འབྲས་བརྒྱ་ཆ།"

#: models.py:142
msgid "This assigns an integer value representing the weight of this problem"
msgstr "འདིས་དཀའ་ངལ་འདིའི་ལྗིད་ཚད་མཚོན་པའི་ཧྲིལ་གྲངས་རིན་ཐང་སྤྲོད་ཀྱི་ཡོད།"

#: models.py:150
msgid "Draft Received Message"
msgstr "ཟིན་བྲིས་འབྱོར་བྱུང་།"

#: models.py:152
msgid "This is the message students will see upon submitting a draft response"
msgstr "འདི་ནི་སློབ་མ་ཚོས་ལན་འདེབས་ཟིན་བྲིས་ཕུལ་སྐབས་མཐོང་བའི་བརྡ་འཕྲིན་ཡིན།"

#: templates/view.html:15
msgid ""
"Allow my response to possibly be visible by other learners after submitting "
"their response"
msgstr "ངའི་ལན་དེ་སློབ་མ་གཞན་གྱིས་ལན་འདེབས་བྱས་རྗེས་མཐོང་ཐུབ།"

#: templates/view.html:21 templates/view.html:24
msgid "Checking..."
msgstr "ཞིབ་བཤེར་བྱེད་བཞིན་ཡོད།"

#: templates/view.html:21 templates/view.html:22
msgid "Submit"
msgstr "འབུལ་སྤྲོད།"

#: templates/view.html:24 templates/view.html:25
msgid "Save"
msgstr "ཉར་ཚགས།"

#: templates/view.html:34
msgid "Hide"
msgstr "སྤེད།"

#: templates/view.html:35
msgid "Show"
msgstr "སྟོན།"

#: templates/view.html:36
msgid "peer responses"
msgstr "མཉམ་རོགས་ཀྱི་ལན་འདེབས།"

#: templates/view.html:38
msgid "Submissions by others"
msgstr "གཞན་གྱིས་ཕུལ་བའི་ཡིག་ཆ།"

#: templates/view.html:39 templates/view.html:43
msgid "No responses to show at this time"
msgstr "མིག་སྔར་སྟོན་པའི་ལན་འདེབས་གང་ཡང་མེད།"

#: views.py:126
#, python-brace-format
msgid "{weight} point possible"
msgid_plural "{weight} points possible"
msgstr[0] "{weight} སྐར་གྲངས་འབྱུང་སྲིད།"

#: views.py:134
#, python-brace-format
msgid "{score_string}/{weight} point"
msgid_plural "{score_string}/{weight} points"
msgstr[0] "{score_string} ༠ / {weight} ཛ༠ སྐར་མ།"

#: views.py:147
#, python-brace-format
msgid "You have used {count_attempts} of {max_attempts} submission"
msgid_plural "You have used {count_attempts} of {max_attempts} submissions"
msgstr[0] ""
"ཁྱོད་ཀྱིས་ {max_attempts} ཡི་འབུལ་བ {count_attempts} བེད་སྤྱོད་བྱས།"

#: views.py:171
#, python-brace-format
msgid "Your response must be between {min} and {max} word."
msgid_plural "Your response must be between {min} and {max} words."
msgstr[0] "ཁྱེད་ཀྱི་ལན་དེ་ {min} དང་ {max} བར་ཡིན་དགོས།"

#: views.py:267
#, python-brace-format
msgid "Invalid Word Count. {word_count_message}"
msgstr "མིང་ཚིག་གྲངས་འབོར། {word_count_message}"
